                                                                                                MODULE SIEVE;                                                   VAR I,J,K,L,N: INTEGER;                                         FLAGS:ARRAY [0..8190] OF INTEGER;                               BEGIN                                                              FOR I:=0 TO 8190 DO FLAGS[I]:=1 END;                            N:=0;                                                           FOR I:=0 TO 8190                                                DO IF FLAGS[I]=1                                                   THEN K:=2*I+3; L:=K+I;                                               WHILE L<=8190                                                   DO FLAGS[L]:=0; L:=L+K                                          END; N:=N+1                                                END                                                          END                                                          END SIEVE .                                                                                                                     MODULE INIMENE;                                                 TYPE Inime=RECORD                                                CASE Mees: BOOLEAN OF                                             TRUE: Aukraad: CARDINAL |                                       FALSE: Neiuna: CHAR;                                          END;                                                           END; END INIMENE.                                                                                                                                                                                                                                                                                                                                                                                                                                                                                               