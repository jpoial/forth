                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                MODULE KUUP;                                                    TYPE DATA=RECORD DAY:[1..31];                                                    MONTH: [1..12];                                                 YEAR: CARDINAL ; END ;                         VAR D: DATA;                                                    BEGIN                                                           WITH D DO                                                       DAY:=30; MONTH:=8; YEAR:=1989                                   END;                                                            END KUUP .                                                                                                                                                                                                                                                                                                                                                                                                      