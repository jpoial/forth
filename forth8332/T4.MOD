MODULE T4;
TYPE TTT=ARRAY [0..6] OF CHAR;
VAR XXX:TTT;
CONST KKK='TEXT'; 
PROCEDURE PRINTCARR(YYY:ARRAY OF CHAR);
VAR JJJ:CARDINAL;
BEGIN
     JJJ:=HIGH(YYY);
(*     JJJ:=6; *)
   FORTH
       /$ addr YYY addr JJJ @ 1+ TYPE CR $/
   END
END PRINTCARR;

BEGIN
   XXX:='STRING';
   PRINTCARR(XXX);
   PRINTCARR(KKK);
   PRINTCARR('SQNA')
END T4.